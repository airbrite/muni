{
  "name": "muni",
  "version": "1.1.3",
  "description": "Muni - MongoDB, Express, Backbone based API framework.",
  "main": "index.js",
  "scripts": {
    "lint": "jshint .",
    "pretest": "npm run lint",
    "test": "npm run test-unit",
    "test-unit": "mocha --reporter spec --require test/support/common --bail --check-leaks --recursive test/unit/",
    "test-integration": "mocha --reporter spec --require test/support/common --bail --check-leaks --recursive test/integration/"
  },
  "homepage": "https://github.com/airbrite/muni",
  "repository": {
    "type": "git",
    "url": "https://github.com/airbrite/muni.git"
  },
  "keywords": [
    "mongodb",
    "express",
    "backbone",
    "api"
  ],
  "dependencies": {
    "accounting": "0.4.1",
    "backbone": "1.1.2",
    "bluebird": "~2.9.27",
    "debug": "~2.2.0",
    "express": "~4.12.4",
    "lodash": "~3.9.3",
    "moment": "~2.10.3",
    "mongodb": "~2.0.33",
    "mongodb-objectid-helper": "0.2.0",
    "request": "~2.57.0",
    "urlsafe-base64": "1.0.0",
    "uuid": "~2.0.1",
    "xml2js": "0.4.9"
  },
  "devDependencies": {
    "chai": "*",
    "chai-as-promised": "*",
    "chai-datetime": "*",
    "eyes": "git://github.com/airbrite/eyes.js.git",
